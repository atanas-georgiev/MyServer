@page
@using Microsoft.Extensions.Localization
@using MyServer.Web.Helpers

@inject IStringLocalizer<SharedResource> SharedLocalizer
@{
    ViewData["Title"] = SharedLocalizer["About"];
}

<div class="well">
    <div class="text-center">
        <h1>@SharedLocalizer["About"]</h1>
        <hr />
    </div>
    <div>
        @await Component.InvokeAsync("Content", new { key = "About" })
    </div>
</div>
